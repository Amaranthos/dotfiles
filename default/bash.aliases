alias chrome='open -a "/Applications/Google Chrome.app"'

alias home='cd ~'
alias reload='source ~/.bash_profile'

alias ll='ls -l'
alias la='ls -a'
alias lla='ls -al'

alias push='git pull -r && git push'

alias path="echo \"${PATH//:/$'\n'}\""

type git-together >/dev/null 2>&1 && alias gt=git-together

alias yws='yarn workspace'